<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>PokerAgentTest</name>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/2">
            <reference>
                <foreign-project>PokerApplication</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.PokerApplication}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
                <properties>
                    <property name="config.active"/>
                </properties>
            </reference>
            <reference>
                <foreign-project>PokerApplication</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.PokerApplication}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar.DefaultConfigurationUsingExternalFolderContents</id>
                <properties>
                    <property name="config.active">DefaultConfigurationUsingExternalFolderContents</property>
                </properties>
            </reference>
            <reference>
                <foreign-project>PokerApplication</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.PokerApplication}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar.OptimizedConfiguration</id>
                <properties>
                    <property name="config.active">OptimizedConfiguration</property>
                </properties>
            </reference>
            <reference>
                <foreign-project>PokerApplication</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.PokerApplication}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar.OptimizedConfigurationUsingExternalFolderContents</id>
                <properties>
                    <property name="config.active">OptimizedConfigurationUsingExternalFolderContents</property>
                </properties>
            </reference>
        </references>
    </configuration>
</project>
